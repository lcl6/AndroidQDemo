{
  "easyGoVersion": "1.0",
  "client": "com.example.androidqdemo",
  "logicEntities": [
    {
      "head": {
        "function": "magicwindow",
        "required": "true"
      },
      "body": {
        "mode": "1",
        "activityPairs": [
          {
            "from": "com.example.androidqdemo.ac.ZxingScanTestActivity",
            "to": "com.example.androidqdemo.ac.CameraXTestActivity"
          },
          {
            "from": "com.example.androidqdemo.ac.CameraXTestActivity",
            "to": "*"
          }
        ],
        "Activities": [
          {
            "name": "com.example.androidqdemo.ac.ZxingScanTestActivity",
            "defaultFullScreen": "true"
          },
          {
            "name": "com.example.androidqdemo.ac.CameraXTestActivity",
            "lockSide": "primary"
          }


        ],
        "transActivities": ["com.example.androidqdemo.ac.CameraXTestActivity"],
        "UX": {
          "supportRotationUxCompat": "true",
          "isDraggable": "true"
        }
      }
    }
  ]
}